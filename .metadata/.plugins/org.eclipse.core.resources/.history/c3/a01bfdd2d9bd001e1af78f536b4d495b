#include <stdint.h>

#define RCC_BASE_ADDR    0x40023800UL
#define GPIOA_BASE_ADDR  0x40020000UL

#define RCC_CFGR_OFF    0x08UL
#define RCC_CR_OFF      0x00UL
#define GPIO_AFRL_OFF   0x20UL
#define GPIO_AFRH_OFF   0x24UL
#define RCC_AHB1_OFF    0x30UL

#define RCC_CFGR_ADDR   (RCC_BASE_ADDR + RCC_CFGR_OFF)
#define RCC_CR_ADDR     (RCC_BASE_ADDR + RCC_CR_OFF)
#define RCC_AHB1_ADDR   (RCC_BASE_ADDR + RCC_AHB1_OFF)
#define RCC_AFRH_ADDR   (GPIOA_BASE_ADDR + GPIO_AFRH_OFF)

int main(void)
{
	uint32_t* pRccCR = (uint32_t*) RCC_CR_ADDR;
	uint32_t* pRCCCFGR = (uint32_t*) RCC_CFGR_ADDR;
	*pRccCR |= (1 << 16);

	while ( ! (*pRccCR & (1 << 17)));

	*pRCCCFGR |= (1 << 0);


	*pRCCCFGR |= (1 << 22);
	//Configure MCO1 prescaler // divisor as 4
	*pRCCCFGR |= ( 1 << 25);
	*pRCCCFGR |= ( 1 << 26);

	uint32_t *pRCCAHB1Enr = (uint32_t*)RCC_AHB1_ADDR;
	*pRCCAHB1Enr |= (1 << 0);


	uint32_t* pGPIOAModeReg = (uint32_t*) GPIOA_BASE_ADDR;
	*pGPIOAModeReg &= ~( 0x3 << 16); //clear
	*pGPIOAModeReg |= (0x2 << 16);

	uint32_t* pGPIOAFRH = (uint32_t*) GPIOA_AFRH_ADDR;
	*pGPIOAFRH |= (0x8 << 0);



}
