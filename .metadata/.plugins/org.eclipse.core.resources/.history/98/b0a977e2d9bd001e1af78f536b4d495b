#include <stdint.h>

#define RCC_BASE_ADDR    0x40023800UL
#define ADC_BASE_ADDR    0x40012000UL


#define ADC_CR1_OFF     0x04UL
#define RCC_CFGR_OFF    0x08UL
#define RCC_APB2_OFF    0x44UL



#define ADC_CR1_ADDR    (ADC_BASE_ADDR + ADC_CR1_OFF)
#define RCC_APB2_ADDR   (RCC_BASE_ADDR + RCC_APB2_OFF)

int main(void)
{
    uint32_t *pAdcr1Reg = (uint32_t*) ADC_CR1_ADDR;
    uint32_t *pRCC_APB2 = (uint32_t*) RCC_APB2_ADDR;

    *pRCC_APB2 |= (1<<8);
    *pAdcr1Reg |= (1 << 8);

}
